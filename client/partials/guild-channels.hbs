<style>
    #guild-channels {
        display: flex;
        flex-direction: column;
        flex-wrap: nowrap;
        flex: 0 0 240px;
        width: 240px;
        background-color: var(--bg-overlay-3, var(--background-secondary));
    }

    #channels-list>svg {
        fill: var(--background-primary);
        padding: 16px;
    }

    #channels-list {
        flex: 1 1;
        overflow-y: scroll;
    }

    #channels-top {
        padding: 0 10px;
        box-shadow: var(--elevation-low);
        flex: 0 0 48px;
        font-family: var(--font-display);
        font-weight: 500;
        padding: 12px 16px;
        box-sizing: border-box;
        transition: background-color.1s linear;
        box-shadow: var(--elevation-low);
    }

    #channels-top>div {
        display: flex;
        align-items: center;
        color: var(--header-primary);
        justify-content: space-between;
        height: 24px;
    }

    #channels-top>div>h2 {
        z-index: 1;
        margin-right: auto;
        color: inherit;
        font-family: var(--font-primary);
        font-size: 16px;
        line-height: 1.25;
        font-weight: 600;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
        min-width: 0;
    }

    #channels-top>div>svg {
        position: relative;
        opacity: .8;
        transition: transform.2s ease-out,stroke-dasharray.2s ease-out;
        margin-left: 4px;
        min-width: 18px;
    }
</style>
<div id="guild-channels">
    <div id="channels-top">
        <div>
            <h2>
                {{guild.name}}
            </h2>
            <!--
            <svg width="18" height="18"><g fill="none" fill-rule="evenodd"><path d="M0 0h18v18H0"></path><path stroke="currentColor" d="M4.5 4.5l9 9" stroke-linecap="round"></path><path stroke="currentColor" d="M13.5 4.5l-9 9" stroke-linecap="round"></path></g></svg>
            -->
        </div>
    </div>

    <div id="channels-list">
        <style>
            #channels-list>a {
                margin: 1px 0;
                overflow: visible;
                position: relative;
                margin-left: 8px;
                cursor: pointer;
                position: relative;
                box-sizing: border-box;
                padding: 6px 8px;
                border-radius: var(--radius-xs);
                display: flex;
                flex-direction: column;
            }

            #channels-list>a:hover {
                background: var(--bg-overlay-hover,var(--background-modifier-hover));
            }
            
            #channels-list>a:active {
                background: var(--bg-overlay-active,var(--background-modifier-active));
            }

            #channels-list>a.selected {
                background: var(--bg-overlay-selected, var(--background-modifier-selected));
            }

            #channels-list>a::before {
                content: "";
                display: block;
                position: absolute;
                top: -1px;
                right: 0;
                bottom: -1px;
                left: 0;
            }

            #channels-list>a>div {
                position: relative;
                display: flex;
                justify-content: center;
                align-items: center;
            }

            #channels-list>a>div>div:nth-child(1) {
                position: relative;
                margin-right: 6px;
            }

            #channels-list>a>div>div:nth-child(2) {
                color: var(--channels-default);
                white-space: nowrap;
                overflow: hidden;
                text-overflow: ellipsis;
                font-size: 16px;
                line-height: 20px;
                font-weight: 500;
                flex: 1 1 auto;
            }

            #channels-list>a.selected>div>div:nth-child(2) {
                color: var(--interactive-active);
                font-weight: 500;
                font-weight: var(--font-weight-semibold-1x-light-theme,500);
            }

            #channels-list>a>div>div:nth-child(1)>svg {
                display: block;
                width: 20px;
                height: 20px;
                flex: 0 0 auto;
                color: var(--channel-icon);
            }
        </style>
        {{#each guild.channels}}
        {{#notequal this.type 4}}
        <a href="/channels/{{../guild.id}}/{{this.id}}"
            {{#equal this.id ../channel.id}}
            class="selected"
            {{/equal}}
        >
            <div>
                <div>
                    {{#switch this.type}}

                    {{#case 0}}
                    <svg role="img" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none"
                        viewBox="0 0 24 24">
                        <path fill="currentColor" fill-rule="evenodd"
                            d="M10.99 3.16A1 1 0 1 0 9 2.84L8.15 8H4a1 1 0 0 0 0 2h3.82l-.67 4H3a1 1 0 1 0 0 2h3.82l-.8 4.84a1 1 0 0 0 1.97.32L8.85 16h4.97l-.8 4.84a1 1 0 0 0 1.97.32l.86-5.16H20a1 1 0 1 0 0-2h-3.82l.67-4H21a1 1 0 1 0 0-2h-3.82l.8-4.84a1 1 0 1 0-1.97-.32L15.15 8h-4.97l.8-4.84ZM14.15 14l.67-4H9.85l-.67 4h4.97Z"
                            clip-rule="evenodd"></path>
                    </svg>
                    {{/case}}

                    {{#case 2}}
                    <svg role="img" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none"
                        viewBox="0 0 24 24">
                        <path fill="currentColor"
                            d="M12 3a1 1 0 0 0-1-1h-.06a1 1 0 0 0-.74.32L5.92 7H3a1 1 0 0 0-1 1v8a1 1 0 0 0 1 1h2.92l4.28 4.68a1 1 0 0 0 .74.32H11a1 1 0 0 0 1-1V3ZM15.1 20.75c-.58.14-1.1-.33-1.1-.92v-.03c0-.5.37-.92.85-1.05a7 7 0 0 0 0-13.5A1.11 1.11 0 0 1 14 4.2v-.03c0-.6.52-1.06 1.1-.92a9 9 0 0 1 0 17.5Z"
                            class=""></path>
                        <path fill="currentColor"
                            d="M15.16 16.51c-.57.28-1.16-.2-1.16-.83v-.14c0-.43.28-.8.63-1.02a3 3 0 0 0 0-5.04c-.35-.23-.63-.6-.63-1.02v-.14c0-.63.59-1.1 1.16-.83a5 5 0 0 1 0 9.02Z">
                        </path>
                    </svg>
                    {{/case}}

                    {{#default}}
                    {{{this.type}}}
                    {{/default}}

                    {{/switch}}
                </div>
                <div>
                    {{this.name}}
                </div>
            </div>
        </a>
        {{/notequal}}
        {{/each}}
    </div>

    {{>channels-status}}
</div>